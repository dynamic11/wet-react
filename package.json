{
  "name": "@dynamic11/wet-react",
  "author": "ISED",
  "license": "ISC",
  "version": "0.2.0",
  "description": "React component library for WET-BOEW components compatible with the GC-Web theme",
  "keywords": [
    "react",
    "react-component",
    "bootstrap",
    "WET",
    "WET-BOEW",
    "accessibility",
    "GC-Web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dynamic11/wet-react.git"
  },
  "bugs": {
    "url": "https://github.com/dynamic11/wet-react/issues"
  },
  "scripts": {
    "rollup": "rollup -c",
    "test": "jest --coverage",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "npx prettier . --check",
    "prettier:fix": "npx prettier . --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=<need-to-add>",
    "prepare": "husky install"
